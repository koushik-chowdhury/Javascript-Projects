{
    "manifest_version": 2,
    "name": "Image Downloader",
    "version": "1.0",
    "description": "Download images in your preferred format",
    "permissions": [
      "activeTab"
    ],
    "background": {
        "scripts": ["script.js"]
      },
    "browser_action": {
      "default_popup": "index.html"
    }
  }
  